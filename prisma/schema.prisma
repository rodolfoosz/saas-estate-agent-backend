generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  fullName   String
  email      String   @unique
  cpf        String
  birthDate  DateTime
  phone      String
  address    String
  password   String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
